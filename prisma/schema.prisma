generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model artwork_data {
  id    Int    @id(map: "Artwork_pkey") @unique(map: "Artwork_id_key") @default(autoincrement())
  s3Url String
  song  song?
}

model melon_crawling {
  song_id String   @id
  title   String
  artists String[]
  url     String
}

model song {
  id            Int           @id(map: "Song_pkey") @default(autoincrement())
  createdAt     DateTime      @default(now())
  title         String
  artist        String
  bgColor       String?
  releaseDate   String?
  spotifyUrl    String?
  appleMusicUrl String?
  youtubeUrl    String?
  popularity    Int?
  artworkId     Int?          @unique(map: "Song_artworkId_key")
  rawArtwork    String?
  melonUrl      String?
  bugsUrl       String?
  naverVibeUrl  String?
  floUrl        String?
  genieUrl      String?
  artwork_data  artwork_data? @relation(fields: [artworkId], references: [id], map: "Song_artworkId_fkey")

  @@unique([title, artist], map: "Song_title_artist_key")
}
